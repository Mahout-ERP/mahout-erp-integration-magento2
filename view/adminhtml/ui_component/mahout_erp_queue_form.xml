<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">mahout_erp_queue_form.queue_form_data_source</item>
		</item>
		<item name="label" translate="true" xsi:type="string">General Information</item>
		<item name="template" xsi:type="string">templates/form/collapsible</item>
	</argument>
	<settings>
		<buttons>
			<button class="Mahout\ERP\Block\Adminhtml\Queue\Edit\BackButton" name="back"/>
			<button class="Mahout\ERP\Block\Adminhtml\Queue\Edit\DeleteButton" name="delete"/>
			<button class="Mahout\ERP\Block\Adminhtml\Queue\Edit\SaveButton" name="save"/>
			<button class="Mahout\ERP\Block\Adminhtml\Queue\Edit\SaveAndContinueButton" name="save_and_continue"/>
		</buttons>
		<namespace>mahout_erp_queue_form</namespace>
		<dataScope>data</dataScope>
		<deps>
			<dep>mahout_erp_queue_form.queue_form_data_source</dep>
		</deps>
	</settings>
	<dataSource name="queue_form_data_source">
		<argument name="data" xsi:type="array">
			<item name="js_config" xsi:type="array">
				<item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
			</item>
		</argument>
		<settings>
			<submitUrl path="*/*/save"/>
		</settings>
		<dataProvider class="Mahout\ERP\Model\Queue\DataProvider" name="queue_form_data_source">
			<settings>
				<requestFieldName>queue_id</requestFieldName>
				<primaryFieldName>queue_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<fieldset name="general">
		<settings>
			<label>General</label>
		</settings>
		<field formElement="input" name="request_entity_id" sortOrder="20">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Queue</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">request_entity_id</label>
				<dataScope>request_entity_id</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="request_method" sortOrder="30">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Queue</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">request_method</label>
				<dataScope>request_method</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="textarea" name="request_data" sortOrder="40">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Queue</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">request_data</label>
				<dataScope>request_data</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="request_result" sortOrder="50">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Queue</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">request_result</label>
				<dataScope>request_result</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="request_delete" sortOrder="60">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Queue</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">request_delete</label>
				<dataScope>request_delete</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="request_runs" sortOrder="70">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Queue</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">request_runs</label>
				<dataScope>request_runs</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="status" sortOrder="80">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Queue</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">status</label>
				<dataScope>status</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="textarea" name="response" sortOrder="90">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Queue</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">response</label>
				<dataScope>response</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="date" name="created_at" sortOrder="100">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Queue</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">created_at</label>
				<dataScope>created_at</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="date" name="updated_at" sortOrder="110">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Queue</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">updated_at</label>
				<dataScope>updated_at</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
	</fieldset>
</form>
